<idea-plugin>

  <extensions defaultExtensionNs="com.intellij">
    <applicationService serviceImplementation="com.jetbrains.edu.learning.stepik.alt.HyperskillSettings"/>
    <httpRequestHandler implementation="com.jetbrains.edu.learning.stepik.alt.HyperskillOAuthRestService"/>
  </extensions>
  <extensions defaultExtensionNs="Educational">
    <optionsProvider instance="com.jetbrains.edu.learning.stepik.alt.HyperskillOptions"/>
    <coursesProvider implementation="com.jetbrains.edu.learning.stepik.alt.HyperskillCourseProvider"/>
    <configurator implementationClass="com.jetbrains.edu.learning.stepik.alt.HyperskillConfigurator" language="Hyperskill-JAVA"/>
  </extensions>

  <application-components>
    <component>
      <implementation-class>com.jetbrains.edu.learning.stepik.alt.HyperskillApplicationComponent</implementation-class>
    </component>
  </application-components>
</idea-plugin>